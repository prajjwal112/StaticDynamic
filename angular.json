{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "staticWindow": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/static-window",
      "sourceRoot": "projects/static-window/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/staticWindow",
            "index": "projects/static-window/src/index.html",
            "main": "projects/static-window/src/main.ts",
            "polyfills": "projects/static-window/src/polyfills.ts",
            "tsConfig": "projects/static-window/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/@salesforce-ux/design-system/assets/",
                "output": "/assets"
              },
              "projects/static-window/src/favicon.ico",
              "projects/static-window/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/static-window/src/styles.scss",
              "node_modules/@salesforce-ux/design-system/assets/styles/salesforce-lightning-design-system.min.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/static-window/src/environments/environment.ts",
                  "with": "projects/static-window/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "staticWindow:build:production"
            },
            "development": {
              "browserTarget": "staticWindow:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "staticWindow:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/static-window/src/test.ts",
            "polyfills": "projects/static-window/src/polyfills.ts",
            "tsConfig": "projects/static-window/tsconfig.spec.json",
            "karmaConfig": "projects/static-window/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/@salesforce-ux/design-system/assets/",
                "output": "/assets"
              },
              "projects/static-window/src/favicon.ico",
              "projects/static-window/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/static-window/src/styles.scss",
              "node_modules/@salesforce-ux/design-system/assets/styles/salesforce-lightning-design-system.min.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "dynamicWindow": {
      "projectType": "application",
